    "90.ibShadowedToolTip" : {
      "comment" : "Class = \"NSButton\"; ibShadowedToolTip = \"Use this to change the directory that games and their data are kept in. You will need to have 'Keep games organized' turned on to make use of this.\"; ObjectID = \"90\";",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "needs_review",
            "value" : "Verwenden Sie dies, um das Verzeichnis zu ändern, in dem Spiele und ihre Daten aufbewahrt werden. Sie müssen \"Spiele organisiert halten\" aktiviert haben, um dies zu nutzen."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Use this to change the directory that games and their data are kept in. You will need to have 'Keep games organized' turned on to make use of this."
          }
        },
        "en-GB" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Use this to change the directory that games and their data are kept in. You will need to have 'Keep games organised' turned on to make use of this."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "needs_review",
            "value" : "Utilisez-le pour modifier le répertoire dans lequel les jeux et leurs données sont conservés. Vous devrez avoir \"Garder les jeux organisés\" activé pour en faire usage."
          }
        },
        "ja" : {
          "stringUnit" : {
            "state" : "needs_review",
            "value" : "これを使用して、ゲームとそのデータが保持されているディレクトリを変更します。これを利用するには、「ゲームを整理する」をオンにする必要があります。"
          }
        }
      }
    },
