    "25.ibShadowedToolTip" : {
      "comment" : "Class = \"NSTextField\"; ibShadowedToolTip = \"Set this to change the interpreter revision reported to games. Most games just report this at the start and make no further use of it.\"; ObjectID = \"25\";",
      "extractionState" : "extracted_with_value",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "needs_review",
            "value" : "Stellen Sie dies ein, um die Interpreterrevision zu ändern, die den Spielen gemeldet wird. Die meisten Spiele melden dies einfach zu Beginn und nutzen es nicht weiter."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "new",
            "value" : "Set this to change the interpreter revision reported to games. Most games just report this at the start and make no further use of it."
          }
        },
        "en-GB" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Set this to change the interpreter revision reported to games. Most games just report this at the start and make no further use of it."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "needs_review",
            "value" : "Définissez ceci pour modifier la révision de l'interprète signalée aux jeux. La plupart des jeux ne font que le signaler au début et ne l'utilisent plus."
          }
        },
        "ja" : {
          "stringUnit" : {
            "state" : "needs_review",
            "value" : "ゲームに報告されたインタプリタのリビジョンを変更するには、これを設定します。ほとんどのゲームは最初にこれを報告するだけで、それ以上は使用しません。"
          }
        }
      }
    },
