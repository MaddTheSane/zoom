    "48.ibShadowedToolTip" : {
      "comment" : "Class = \"NSPopUpButton\"; ibShadowedToolTip = \"Sets the interpreter type reported to the game. Most modern games ignore this setting, but this can change the behavior of many Infocom games.\"; ObjectID = \"48\";",
      "extractionState" : "extracted_with_value",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "needs_review",
            "value" : "Legt den Interpretertyp fest, der dem Spiel gemeldet wird. Die meisten modernen Spiele ignorieren diese Einstellung, aber dies kann das Verhalten vieler Infocom-Spiele ändern."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "new",
            "value" : "Sets the interpreter type reported to the game. Most modern games ignore this setting, but this can change the behavior of many Infocom games."
          }
        },
        "en-GB" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Sets the interpreter type reported to the game. Most modern games ignore this setting, but this can change the behaviour of many Infocom games."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "needs_review",
            "value" : "Définit le type d'interprète signalé au jeu. La plupart des jeux modernes ignorent ce paramètre, mais cela peut changer le comportement de nombreux jeux Infocom."
          }
        },
        "ja" : {
          "stringUnit" : {
            "state" : "needs_review",
            "value" : "ゲームに報告されたインタプリタタイプを設定します。ほとんどの現代のゲームはこの設定を無視しますが、これは多くのインフォコムゲームの動作を変える可能性があります。"
          }
        }
      }
    },
